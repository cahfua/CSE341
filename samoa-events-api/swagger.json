{
  "openapi": "3.0.0",
  "info": {
    "title": "Samoa Community Events API",
    "version": "1.0.0",
    "description": "API for managing Samoan community events and RSVPs"
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Local"
    },
    {
      "url": "https://samoa-events-api.onrender.com",
      "description": "Render"
    }
  ],
  "paths": {
    "/events": {
      "get": {
        "summary": "Get all events",
        "responses": {
          "200": {
            "description": "Returns all events"
          }
        }
      }
    },
    "/rsvps": {
      "get": {
        "summary": "Get all RSVPs",
        "responses": {
          "200": {
            "description": "Returns all RSVPs"
          }
        }
      }
    }
  }
}
